{
  "extends": "airbnb",
  "rules": {
    "arrow-parens": 0,
    "global-require": 0,
    "import/no-dynamic-require": 0,
    "no-await-in-loop": 0,
    "brace-style": 0,
    "comma-dangle": 0,
    "class-methods-use-this": 0,
    "key-spacing": 0,
    "no-multi-assign": 0,
    "one-var": 0,
    "prefer-rest-params": 0,
    "strict": 0,
    "no-console": 0,
    "guard-for-in": 0,
    "no-restricted-syntax": 0,
    "no-underscore-dangle": 0,
    "no-param-reassign": 0
  }
}
